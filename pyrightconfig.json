{
  "include": [
    "src",
    "tests"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/.*"
  ],
  "ignore": [],
  "defineConstant": {
    "DEBUG": true
  },
  "stubPath": "typings",
  "typeCheckingMode": "basic",
  "useLibraryCodeForTypes": true,
  "reportMissingImports": "warning",
  "reportMissingTypeStubs": "none",
  "pythonVersion": "3.8",
  "pythonPlatform": "Darwin",
  "executionEnvironments": [
    {
      "root": "src",
      "extraPaths": [
        "src"
      ]
    },
    {
      "root": "tests",
      "extraPaths": [
        "src"
      ]
    }
  ]
}
